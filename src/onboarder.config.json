{
  "ignorePatterns": [
    "**/node_modules/**",
    "**/out/**",
    "**/dist/**",
    "**/.vscode/**",
    "**/.git/**",
    "**/.svn/**",
    "**/*.wasm",
    "**/.hg/**",
    "**/CVS/**",
    "**/.DS_Store/**",
    "**/thumbs.db/**",
    "**/venv/**",
    "**/__pycache__/**",
    "**/*.pyc",
    "**/*.pyo",
    "**/*.class",
    "**/*.jar",
    "**/*.war",
    "**/*.ear",
    "**/*.log",
    "**/*.tmp",
    "**/.gitignore",
    "**/go.sum",
    "**/go.mod",
    "**/*.exe",
    "**/.next/**"
  ],
  "languageMapping": {
    ".ts": "TypeScript",
    ".tsx": "TypeScript",
    ".js": "JavaScript",
    ".jsx": "JavaScript",
    ".json": "JSON",
    ".jsonc": "JSONC",
    ".md": "Markdown",
    ".css": "CSS",
    ".scss": "SCSS",
    ".less": "Less",
    ".html": "HTML",
    ".py": "Python",
    ".java": "Java",
    ".cs": "C#",
    ".c": "C",
    ".h": "C/C++ Header",
    ".cpp": "C++",
    ".hpp": "C++",
    ".go": "Go",
    ".rs": "Rust",
    ".php": "PHP",
    ".rb": "Ruby",
    ".vue": "Vue",
    ".svelte": "Svelte",
    ".yml": "YAML",
    ".yaml": "YAML",
    ".xml": "XML",
    ".sh": "Shell",
    ".bash": "Shell",
    ".ps1": "PowerShell",
    ".sql": "SQL",
    ".swift": "Swift",
    ".kt": "Kotlin",
    ".kts": "Kotlin Script",
    ".dart": "Dart",
    ".lua": "Lua",
    ".pl": "Perl",
    ".pm": "Perl",
    ".r": "R",
    ".m": "Objective-C",
    ".mm": "Objective-C++",
    ".txt": "Text",
    ".toml": "TOML",
    ".lock": "Lockfile",
    ".cfg": "Config",
    ".ini": "INI",
    ".env": "Environment",
    ".svg": "XML"
  },
  "projectNameDetectors": [
    {
      "file": "package.json",
      "parser": "json",
      "key": "name"
    },
    {
      "file": "composer.json",
      "parser": "json",
      "key": "name"
    },
    {
      "file": "pyproject.toml",
      "parser": "regex",
      "pattern": "name\\s*=\\s*\\\"(.*?)\\\""
    },
    {
      "file": "pom.xml",
      "parser": "regex",
      "pattern": "<artifactId>(.*?)<\\/artifactId>"
    },
    {
      "file": "build.gradle",
      "parser": "regex",
      "pattern": "rootProject\\.name\\s*=\\s*['\"](.*?)['\"]"
    },
    {
      "file": "build.gradle.kts",
      "parser": "regex",
      "pattern": "rootProject\\.name\\s*=\\s*\\\"(.*?)\\\""
    },
    {
      "file": "Cargo.toml",
      "parser": "regex",
      "pattern": "^\\s*name\\s*=\\s*\\\"(.*?)\\\""
    },
    {
      "file": "CMakeLists.txt",
      "parser": "regex",
      "pattern": "project\\((.*?)\\)"
    },
    {
      "file": "Makefile.am",
      "parser": "regex",
      "pattern": "AM_INIT_AUTOMAKE\\((.*?),(.*?)\\)"
    },
    {
      "file": "*.csproj",
      "parser": "regex",
      "pattern": "<AssemblyName>(.*?)<\\/AssemblyName>"
    },
    {
      "file": "project.clj",
      "parser": "regex",
      "pattern": "\\(defproject\\s+(.*?)\\s+"
    }
  ]
}